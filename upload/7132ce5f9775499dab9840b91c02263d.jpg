    "import base64\n",
    "import hashlib\n",
    "\n",
    "def encode(t):\n",
    "    HASH_SALT = \"meow\"\n",
    "    return base64.b64encode(hashlib.md5(\n",
    "        (str(t) + HASH_SALT).encode('utf-8')).digest(), altchars=b\"-_\")[:4].decode(\"utf=-8\")\n",
    "\n",
    "print(len(set([encode(i) for i in range(10000)])))"
   ]
